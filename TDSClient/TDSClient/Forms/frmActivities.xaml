        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:globalres="clr-namespace:TDSClient.Properties"
        Title="{x:Static  globalres:Resources.strActivites }" Height="248.8" Width="426.4" FontFamily="Global User Interface" FontSize="11" ResizeMode="NoResize" WindowStyle="ToolWindow" ShowInTaskbar="False" >
