        <DataGrid x:Name="dtGridActivities" Background="Transparent"   RowBackground="Transparent"  Width="395"  HorizontalAlignment="Left" Margin="7,0,0,39"    />
        <Button Name="btnNew" Content="{x:Static  globalres:Resources.strbtnNew }" HorizontalAlignment="Left" Margin="172,188,0,0" VerticalAlignment="Top" Width="75" Click="btnNew_Click"/>
        <Button x:Name="btnEdit" Content="{x:Static globalres:Resources.strbtnEdit}" HorizontalAlignment="Left" Margin="261,188,0,0" VerticalAlignment="Top" Width="75" Click="btnEdit_Click" />
        <Button x:Name="btnDelete" Content="{x:Static globalres:Resources.strbtnDelete}" HorizontalAlignment="Left" Margin="83,188,0,0" VerticalAlignment="Top" Width="75" Click="btnDelete_Click" />
    </Grid>
</Window>
