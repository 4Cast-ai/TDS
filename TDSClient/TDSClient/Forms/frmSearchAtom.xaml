<Window x:Class="TDSClient.Forms.frmSearchAtom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:globalres="clr-namespace:TDSClient.Properties"
        Title="{x:Static globalres:Resources.strToolTipSearch}" Height="149" Width="300"   FontFamily="Global User Interface" FontSize="11" ResizeMode="NoResize" Loaded="Window_Loaded" Unloaded="Window_Unloaded">
    <Grid>
        <ComboBox FontSize="12" Height="20" HorizontalAlignment="Left" IsEditable="True" Margin="63,25,0,0" Name="comboBoxAtom" Text="" VerticalAlignment="Top" Width="192" />
        <Button  Content="{x:Static  globalres:Resources.strZoom}" Height="20" HorizontalAlignment="Left" Margin="145,79,0,0" Name="btnZoom" VerticalAlignment="Top" Width="55" Click="btnZoom_Click" />
        <Button  Content="{x:Static  globalres:Resources.strbtnFind}" Height="20" HorizontalAlignment="Left" Margin="214,79,0,0" Name="buttonFind" VerticalAlignment="Top" Width="55" Click="buttonFind_Click" />

        <Button  Content="{x:Static  globalres:Resources.strbtnCancel}" Height="20" HorizontalAlignment="Left" Margin="75,79,0,0" Name="btnCancel" VerticalAlignment="Top" Width="55" Click="btnCancel_Click" />
        
    </Grid>
</Window>
